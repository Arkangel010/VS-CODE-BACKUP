package rest;

import java.util.LinkedList;
import java.util.ListIterator;

public class Linked {
	public static void main(String[] args) {
		LinkedList<String> ls = new LinkedList<String>();
		ls.add("ramesh");
		ls.add("suresh");
		ls.add("jayesh");
		ls.add("naresh");
		
		ListIterator<String> i = ls.listIterator();
		int y = ls.size();
		for(int z = 0;z<= y;z++)
			ls.get(z); 
		for (int ele : ls)
			System.out.println(ele);
		ls.addFirst("mahesh");
		ls.addLast("jignesh");
		ListIterator<String> q = ls.listIterator();
		while(q.hasNext())
			System.out.println(q.next());
		
			
			
		
	}
	
}
