package salary;

public class Salary {
	public static void main(String[] args) {

		double[] data = { 23500.0, 25080.0, 28760.0, 22340.0, 19890.0 };
		double z = 0;
		double y;

		for (int i = 0; i <= data.length; i++) {
			y = data[i];
			z = z + y;

		}
		System.out.println(z / data.length);

	}

}
